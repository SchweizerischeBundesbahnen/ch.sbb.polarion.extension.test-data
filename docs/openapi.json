{
  "openapi": "3.0.1",
  "info": {
    "title": "Test Data REST API",
    "version": "v1"
  },
  "paths": {
    "/api/context": {
      "get": {
        "operationId": "getContext",
        "responses": {
          "default": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Context"
                }
              }
            },
            "description": "Context information"
          }
        },
        "summary": "Returns basic context information of Polarion's extension",
        "tags": [
          "Extension Information"
        ]
      }
    },
    "/api/projects/{projectId}/spaces/{spaceId}/documents/{documentName}": {
      "post": {
        "operationId": "createDocumentWithGeneratedWorkItems",
        "parameters": [
          {
            "in": "path",
            "name": "projectId",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "path",
            "name": "spaceId",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "path",
            "name": "documentName",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "quantity",
            "schema": {
              "default": 100,
              "format": "int32",
              "type": "integer"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Document successfully created with generated work items"
          },
          "409": {
            "description": "Document with this name already exists"
          }
        },
        "summary": "Create document with generated work items",
        "tags": [
          "Test Data"
        ]
      }
    },
    "/api/projects/{projectId}/spaces/{spaceId}/documents/{documentName}/append": {
      "patch": {
        "operationId": "extendDocumentWithGeneratedWorkItems",
        "parameters": [
          {
            "in": "path",
            "name": "projectId",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "path",
            "name": "spaceId",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "path",
            "name": "documentName",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "quantity",
            "schema": {
              "default": 100,
              "format": "int32",
              "type": "integer"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Document successfully appended with generated work items"
          }
        },
        "summary": "Append generated work items to the document",
        "tags": [
          "Test Data"
        ]
      }
    },
    "/api/projects/{projectId}/spaces/{spaceId}/documents/{documentName}/change-wi-descriptions": {
      "patch": {
        "operationId": "changeDocumentWorkItemDescriptions",
        "parameters": [
          {
            "in": "path",
            "name": "projectId",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "path",
            "name": "spaceId",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "path",
            "name": "documentName",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "interval",
            "schema": {
              "default": 5,
              "format": "int32",
              "type": "integer"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "WorkItem descriptions successfully changed"
          }
        },
        "summary": "Change WorkItem Descriptions",
        "tags": [
          "Test Data"
        ]
      }
    },
    "/api/projects/{projectId}/spaces/{spaceId}/documents/{documentName}/large": {
      "post": {
        "description": "Creates a document with specified number of pages, each containing large PNG images. Useful for testing document rendering performance with large content.",
        "operationId": "createLargeDocumentWithImages",
        "parameters": [
          {
            "in": "path",
            "name": "projectId",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "path",
            "name": "spaceId",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "path",
            "name": "documentName",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "pagesCount",
            "schema": {
              "default": 200,
              "format": "int32",
              "type": "integer"
            }
          },
          {
            "in": "query",
            "name": "imagesPerPage",
            "schema": {
              "default": 3,
              "format": "int32",
              "type": "integer"
            }
          },
          {
            "in": "query",
            "name": "imageWidth",
            "schema": {
              "default": 1920,
              "format": "int32",
              "type": "integer"
            }
          },
          {
            "in": "query",
            "name": "imageHeight",
            "schema": {
              "default": 1080,
              "format": "int32",
              "type": "integer"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Large document successfully created"
          },
          "400": {
            "description": "Invalid parameters"
          },
          "409": {
            "description": "Document with this name already exists"
          }
        },
        "summary": "Create large document with PNG images",
        "tags": [
          "Test Data"
        ]
      }
    },
    "/api/templates/{projectId}/download": {
      "get": {
        "operationId": "downloadProjectTemplate",
        "parameters": [
          {
            "in": "path",
            "name": "projectId",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "projectGroup",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Template successfully downloaded"
          },
          "400": {
            "description": "Invalid project ID"
          },
          "404": {
            "description": "Template not found"
          }
        },
        "summary": "Download a zipped project template",
        "tags": [
          "Test Data"
        ]
      }
    },
    "/api/templates/{templateId}/hash": {
      "get": {
        "operationId": "getTemplateHash",
        "parameters": [
          {
            "in": "path",
            "name": "templateId",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Template hash successfully retrieved"
          },
          "400": {
            "description": "Invalid template ID"
          },
          "404": {
            "description": "Template or hash not found"
          }
        },
        "summary": "Get the hash of a project template",
        "tags": [
          "Test Data"
        ]
      }
    },
    "/api/templates/{templateId}/{templateHash}": {
      "post": {
        "operationId": "saveProjectTemplate",
        "parameters": [
          {
            "in": "path",
            "name": "templateId",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "path",
            "name": "templateHash",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "properties": {
                  "file": {
                    "$ref": "#/components/schemas/FormDataBodyPart"
                  }
                },
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Template successfully created"
          },
          "400": {
            "description": "Invalid template data or ID"
          }
        },
        "summary": "Upload and save a project template",
        "tags": [
          "Test Data"
        ]
      }
    },
    "/api/version": {
      "get": {
        "operationId": "getVersion",
        "responses": {
          "default": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Version"
                }
              }
            },
            "description": "Version information"
          }
        },
        "summary": "Returns version of Polarion's extension",
        "tags": [
          "Extension Information"
        ]
      }
    }
  },
